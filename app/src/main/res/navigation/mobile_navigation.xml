<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_stat">

    <fragment
        android:id="@+id/navigation_stat"
        android:name="com.driveup.ui.stat.StatFragment"
        android:label="@string/title_stat"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_ride"
        android:name="com.driveup.ui.ride.RideFragment"
        android:label="@string/title_ride"
        tools:layout="@layout/fragment_ride" />

    <fragment
        android:id="@+id/navigation_importexport"
        android:name="com.driveup.ui.importexport.ImportExportFragment"
        android:label="@string/title_importexport"
        tools:layout="@layout/fragment_import_export" />

</navigation>